<div>
  <%= image_tag current_user.get_profile_image(100,100) %>
  <h3><%= current_user.name %></h3>
  <h3><%= current_user.introduction %></h3>
  <%= link_to "Edit", edit_user_path(current_user.id) %>
</div>
<%= render 'books/new', book: @book %>

<h1>Users</h1>
<table>
 <thead>
   <tr>
    <th>
     image
    </th>
    <th>
     name
    </th>
    <th colspan="3"></th>
   </tr>
  </thead>
 <tbody>
<% @users.each do |user| %>
 <tr>
    <td>
     <%= image_tag user.get_profile_image(50,50) %>
    </td>
    <td>
     <%= user.name %>
    </td>
    <td>
    <%= link_to user_path(user.id) do %>
      Show
    <% end %>
    </td>

  </tr>
  
<% end %>
</tbody>
</table>